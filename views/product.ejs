<script>
		function formval()
		{
			var names = document.getElementById('firstname').value;
			var alpha = /^[a-zA-Z]+$/;
			if(names=="") 
			{
				document.getElementById('fname').innerHTML="Name must be filled out";
				return false;
			}
			else if(!names.match(alpha))
			{
				document.getElementById('fname').innerHTML="Name must have alphabets only";
				return false;
			}
			else
			{
				document.getElementById('fname').innerHTML=" ";
				//return true;
			}

			var prices = document.getElementById('price').value;
			var num = /^[0-9]+$/;
			if(prices=="") 
			{
				document.getElementById('fprice').innerHTML="Price must be filled out";
				return false;
			}
			else if(!prices.match(num))
			{
				document.getElementById('fprice').innerHTML="Price must be numeric only";
				return false;
			}
			else
			{
				document.getElementById('fprice').innerHTML=" ";
				//return true;
			}

			var cats = document.getElementById('cat').value;
			if(cats == 0)
			{
				document.getElementById('cates').innerHTML="Category must be selected";
				return false;
			}
			document.getElementById("hi").submit();
		}
</script>

